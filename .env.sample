# AdGuard VPN CLI Configuration
# 
# NOTE: As of version 1.5.10, username/password authentication has been replaced
# with web-based authentication. The ADGUARD_USERNAME and ADGUARD_PASSWORD 
# variables are no longer used for authentication but are kept for backward 
# compatibility in other configuration aspects.

# Authentication (deprecated for authentication - see docs for web auth process)
ADGUARD_USERNAME="username"
ADGUARD_PASSWORD="password"

# Connection settings
ADGUARD_CONNECTION_LOCATION="JP"
ADGUARD_CONNECTION_TYPE="TUN"

# SOCKS5 proxy settings
ADGUARD_SOCKS5_USERNAME="username"
ADGUARD_SOCKS5_PASSWORD="password"
ADGUARD_SOCKS5_HOST="127.0.0.1"
ADGUARD_SOCKS5_PORT=1080

# Privacy and DNS settings
ADGUARD_SEND_REPORTS=false
ADGUARD_SET_SYSTEM_DNS=false
ADGUARD_USE_CUSTOM_DNS=true
ADGUARD_CUSTOM_DNS="1.1.1.1"

# Kill switch settings
ADGUARD_USE_KILL_SWITCH=true
ADGUARD_USE_KILL_SWITCH_CHECK_INTERVAL=30

# Application settings
ADGUARD_UPDATE_CHANNEL="release"
ADGUARD_SHOW_HINTS="on"
ADGUARD_DEBUG_LOGGING="on"
ADGUARD_SHOW_NOTIFICATIONS="on"
ADGUARD_PROTOCOL="auto"
ADGUARD_POST_QUANTUM="off"
ADGUARD_TELEMETRY=false
ADGUARD_TUN_ROUTING_MODE="AUTO"
ADGUARD_BOUND_IF_OVERRIDE=""